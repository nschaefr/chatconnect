{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
      },
    "source": {
        "include": ["./src"],
        "includePattern": ".jsx$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown",
        "jsdoc-mermaid",
        "better-docs/component",
        "node_modules/better-docs/category"
    ],
    "templates": {
        "better-docs": {
          "name": "My React components"
        }
    },
    "opts": {
        "encoding": "utf8",
        "readme": "../README.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    }
}